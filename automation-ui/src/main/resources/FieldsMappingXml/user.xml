<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2012/03">
  <stream name="users" format="delimited">
	<parser>  
      <property name="delimiter" value="," />
       <property name="lineContinuationCharacter" value="\n" />
    </parser>
	<record name="userDetails" minOccurs="0" maxOccurs="unbounded" class="excel.read.Users">	 
      <field name="email" />
      <field name="firstName" />
      <field name="lastName" />    
	</record>
  </stream>
</beanio>
